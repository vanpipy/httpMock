{
  "name": "httpmock",
  "version": "0.1.0",
  "description": "mock the http request",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --testMatch='**/*.test.ts'",
    "test:e2e": "jest --testMatch='**/*.e2e.ts'",
    "typecheck": "tsc -p ./tsconfig-check.json",
    "build": "webpack build --config ./webpack.config.ts",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanpipy/httpMock.git"
  },
  "keywords": [
    "http",
    "mock"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vanpipy/httpMock/issues"
  },
  "homepage": "https://github.com/vanpipy/httpMock#readme",
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.19.2",
    "get-port": "7.1.0",
    "path-to-regexp": "8.0.0"
  },
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.1",
    "@commitlint/config-conventional": "19.4.1",
    "@eslint/js": "9.9.1",
    "@types/cors": "2.8.17",
    "@types/eslint__js": "8.42.3",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.10.0",
    "@types/supertest": "6.0.2",
    "@types/webpack": "5.28.5",
    "eslint": "9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "9.1.5",
    "jest": "29.7.0",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "typescript-eslint": "8.3.0",
    "webpack": "5.94.0",
    "webpack-cli": "5.1.4"
  },
  "lint-staged": {
    "**/*.{ts,js}": [
      "eslint --fix"
    ]
  }
}
